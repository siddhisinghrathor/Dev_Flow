        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "express-rate-limit": "^7.4.1",
        "express-validator": "^7.2.0",
        "helmet": "^8.0.0",
        "ioredis": "^5.4.1",
        "jsonwebtoken": "^9.0.2",
        "socket.io": "^4.8.1",
        "web-push": "^3.6.7",
        "winston": "^3.17.0",
        "zod": "^3.23.8"
