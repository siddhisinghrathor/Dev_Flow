    "version": "1.0.0",
    "description": "Production-grade backend for DevFlow productivity platform",
    "main": "dist/server.js",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
